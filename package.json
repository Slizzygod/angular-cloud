  "author": "Vladislav Zaharov",
  "description": "Angular cloud - Angular Full Stack project built using Angular, Express, PostgreSQL and Node.",
  "engines": {
    "node": "18.13.*",
    "npm": "8.19.*"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "node ./node_modules/@angular/cli/bin/ng build --configuration development",
    "build-prod": "node ./node_modules/@angular/cli/bin/ng build --configuration production",
    "dev": "concurrently \"ng serve --proxy-config proxy.conf.json --host 0.0.0.0 --open\" \"tsc -w -p server\" \"nodemon -r source-map-support/register dist/server/app.js\"",
    "test": "ng test",
    "lint": "ng lint"
  },
